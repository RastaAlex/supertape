{
    "parser": "babel-eslint",
    "overrides": [{
        "files": ["bin/*.js"],
        "rules": {
            "no-process-exit": "off"
        }
    }],
    "rules": {
            "node/no-unsupported-features/es-syntax": "off"
    },
    "extends": [
        "plugin:putout/recommended",
        "plugin:node/recommended"
    ],
    "plugins": [
        "node",
        "putout"
    ]
}
